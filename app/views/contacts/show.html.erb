<%= page_template page_title: t(".h1-heading") do %>
  <p class="govuk-heading-m">
    <%= t(".case_enquiries.heading") %>
  </p>

  <p class="govuk-body">
    <%= t(".case_enquiries.phone_number") %>
  </p>

  <p class="govuk-heading-m">
    <%= t(".technical_support.heading") %>
  </p>

  <p class="govuk-body">
    <%= t(".technical_support.email_text") %>
    <a href="mailto:">
      <%= t(".technical_support.email_address") %>
    </a>
  </p>

<% end %>
