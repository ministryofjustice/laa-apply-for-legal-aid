<dl class="govuk-summary-list govuk-!-margin-bottom-9">
  <%= check_answer_no_link(
        name: :form_of_service,
        question: t('.form_of_service'),
        answer: @legal_aid_application.lead_proceeding_type.default_level_of_service.name
      ) %>
  <%= check_answer_no_link(
        name: :incurrable_costs,
        question: t('.costs'),
        answer: gds_number_to_currency(@legal_aid_application.default_substantive_cost_limitation,  precision: 0)
      ) %>
  <%= check_answer_no_link(
        name: :allowable_work,
        question: t('.work'),
        answer: @legal_aid_application.substantive_scope_limitation.description
      ) %>
</dl>
